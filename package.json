{
  "private": true,
  "name": "necto-monorepo",
  "version": "0.0.0",
  "scripts": {
    "changeset": "changeset",
    "build": "turbo run build",
    "build:id": "turbo run build --filter=@necto/id",
    "build:color": "turbo run build --filter=@necto/color",
    "build:constants": "turbo run build --filter=@necto/constants",
    "build:handlebars": "turbo run build --filter=@necto/handlebars",
    "build:style-dictionary": "turbo run build --filter=@necto/style-dictionary",
    "test": "pnpm exec vitest",
    "test:color": "pnpm exec vitest --project @necto/color",
    "test:style-dictionary": "pnpm exec vitest --project @necto/style-dictionary"
  },
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "ts-node": "^10.9.2",
    "turbo": "^2.4.4",
    "vitest": "^3.0.9"
  },
  "packageManager": "pnpm@9.15.3"
}
