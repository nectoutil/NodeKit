{
  "private": true,
  "name": "necto-monorepo",
  "version": "0.0.0",
  "scripts": {
    "dev": "turbo run dev",
    "storybook": "concurrently \"pnpm storybook:react\" \"pnpm storybook:vue\"",
    "storybook:react": "storybook dev -p 6006",
    "storybook:vue": "storybook dev -p 6007 -c .storybook-vue",
    "build-storybook": "concurrently \"pnpm build-storybook:react\" \"pnpm build-storybook:vue\"",
    "build-storybook:react": "storybook build",
    "build-storybook:vue": "storybook build -c .storybook-vue",
    "changeset": "changeset",
    "test": "turbo run test",
    "test:visual": "turbo run test:visual",
    "build": "turbo run build",
    "lint": "set DEBUG=biome* && biome lint --write",
    "format": "set DEBUG=biome* && biome format --write",
    "postinstall": "lefthook install && echo 'ðŸš¨ Corinvo CLI will be required soon! Install it now to keep developing smoothly. ðŸš€'",
    "clean": "find packages -type d \\( -name 'node_modules' -o -name '.turbo' -o -name 'dist' -o -name '.dist' \\) -prune -exec rm -rf '{}' + && rm -rf node_modules .turbo dist .dist"
  },
  "license": "MIT",
  "pnpm": {
    "overrides": {
      "@types/react": "19.1.2",
      "@types/react-dom": "19.1.2"
    }
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0-beta.5",
    "@changesets/cli": "^2.28.1",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/addon-interactions": "^8.5.0",
    "@storybook/addon-links": "^8.5.0",
    "@storybook/blocks": "^8.5.0",
    "@storybook/react": "^8.5.0",
    "@storybook/react-vite": "^8.5.0",
    "@storybook/test": "^8.5.0",
    "@storybook/vue3": "^8.5.0",
    "@storybook/vue3-vite": "^8.5.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "commitlint": "^19.8.1",
    "concurrently": "^9.2.1",
    "lefthook": "^1.11.13",
    "markdownlint": "^0.38.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "storybook": "^8.5.0",
    "ts-node": "^10.9.2",
    "turbo": "^2.4.4",
    "vitest": "^3.0.9",
    "vue": "^3.5.13"
  },
  "packageManager": "pnpm@9.15.3"
}
