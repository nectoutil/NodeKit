{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false
    },
    "test": {
      "dependsOn": ["build"]
    },
    "build": {
      "outputs": ["dist/**", "cache/**"]
    },
    "build:@necto-react/hooks": {
      "dependsOn": ["^build:@necto-react/types"]
    },
    "build:@necto/id": {
      "dependsOn": ["^build:@necto/constants"]
    },
    "build:@necto-react/helpers": {
      "dependsOn": ["^build:@necto/dom"]
    }
  }
}