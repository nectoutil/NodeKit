{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "style-dictionary#build": {
      "cache": false,
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "handlebars#build": {
      "dependsOn": ["^build"],
      "outputs": [".dist/**"]
    }
  }
}
