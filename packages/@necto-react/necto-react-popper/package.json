{
  "name": "@necto-react/popper",
  "version": "0.0.1",
  "type": "module",
  "description": "Necto's React library for positioning, popovers, tooltips, and floating elements.",
  "scripts": {
    "test": "vitest",
    "build": "tsup ./src/index.ts --minify --format esm,cjs,iife --dts",
    "dev": "tsup ./src/index.ts --watch --minify --format esm,cjs,iife --dts"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "author": "Corinvo OSS Team",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "jsdom": "^26.1.0",
    "tsup": "^8.4.0"
  },
  "peerDependencies": {
    "react": "^19.1.0"
  },
  "dependencies": {
    "@necto-react/hooks": "workspace:*",
    "@necto-react/types": "workspace:*",
    "@necto/dom": "workspace:*",
    "@necto/popper": "workspace:*",
    "@necto/types": "workspace:*",
    "defu": "^6.1.4"
  }
}
