{
  "name": "@necto-react/components",
  "version": "1.2.15",
  "type": "module",
  "description": "Necto's standard library for providing utility components for React applications.",
  "scripts": {
    "test": "vitest",
    "test:unit": "vitest run",
    "test:visual": "playwright test",
    "test:visual:update": "playwright test --update-snapshots",
    "test:all": "pnpm test:unit && pnpm test:visual",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "author": "Corinvo OSS Team",
  "license": "MIT",
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.49.1",
    "@playwright/test": "^1.49.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.14.1",
    "@vitejs/plugin-react": "^4.3.4",
    "esbuild-plugin-css-module": "^0.1.0",
    "esbuild-sass-plugin": "^3.3.1",
    "postcss": "^8.5.6",
    "postcss-modules": "^6.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sass": "^1.83.4",
    "sass-embedded": "^1.93.2",
    "tsup": "^8.4.0",
    "vitest": "^3.0.9"
  },
  "peerDependencies": {
    "@types/react": "^19.1.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@necto-react/hooks": "workspace:*",
    "@necto/constants": "workspace:*",
    "@necto/dom": "workspace:*",
    "clsx": "^2.1.1",
    "react-icons": "^5.5.0",
    "react-if": "^4.1.5"
  }
}
